@startuml

class Employee {
    - id: int
    - firstname: String
    - lastname: String
    - phoneNumber: String
    - email: String
    - salary: int
    - startDate: LocalDate
    - role: Role
    - company: Company
    + builder(): Builder
}
class Company {
    - id: int
    - companyName: String
    - country: String
    + builder(): Builder
}
enum Role {
    COORDINATOR
    DATA
    ENGINEERING
}
Company "1" --o "0.." Employee
Employee . Role

@enduml
